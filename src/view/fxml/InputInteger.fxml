<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="100.0" prefWidth="258.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.InputIntegerController">
   <children>
      <AnchorPane prefHeight="100.0" prefWidth="31.0" style="-fx-background-color: #47A4E9;">
         <children>
            <JFXButton fx:id="cancel" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" onAction="#cancel" prefHeight="30.0" prefWidth="36.0" text=" ">
               <graphic>
                  <FontAwesomeIconView fill="RED" glyphName="CLOSE" size="20" text="" />
               </graphic>
            </JFXButton>
            <JFXButton fx:id="ok" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" layoutY="70.0" onAction="#setOk" prefHeight="30.0" prefWidth="36.0" text=" ">
               <graphic>
                  <FontAwesomeIconView fill="#37ff37" glyphName="CHECK" size="20" text="" />
               </graphic>
            </JFXButton>
         </children>
      </AnchorPane>
      <VBox alignment="CENTER" layoutX="36.0" prefHeight="100.0" prefWidth="244.0">
         <children>
            <JFXComboBox fx:id="inputInt" prefHeight="25.0" prefWidth="114.0" promptText="Số lượng:" />
         </children>
      </VBox>
   </children>
</AnchorPane>
