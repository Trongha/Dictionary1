                  </AnchorPane>
                  <ListView fx:id="listWord" prefHeight="409.0" prefWidth="237.0" />
               </children>
            </VBox>
            <AnchorPane prefHeight="452.0" prefWidth="282.0">
               <children>
                  <TextArea fx:id="output2" layoutX="14.0" layoutY="70.0" prefHeight="382.0" prefWidth="237.0" />
                  <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" layoutX="58.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </AnchorPane>
            <VBox prefHeight="235.0" prefWidth="45.0">
