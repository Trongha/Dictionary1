<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SearchController">
   <children>
      <HBox layoutX="14.0" layoutY="33.0" prefHeight="452.0" prefWidth="561.0">
         <children>
            <VBox prefHeight="452.0" prefWidth="237.0">
               <children>
                  <AnchorPane prefHeight="50.0" prefWidth="159.0">
                     <children>
                        <Button id="searchButton" fx:id="searchButton" layoutX="169.0" layoutY="24.0" mnemonicParsing="false" onAction="#search" prefHeight="25.0" prefWidth="68.0" text="Search" />
